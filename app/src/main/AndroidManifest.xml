<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.everartstudio">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Cancelled_Order"></activity>
        <activity android:name=".Conform_Orders" />
        <activity
            android:name=".User_Home_Screen"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".Forgot_Password"  android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".product_view" />
        <activity android:name=".View_Product" />
        <activity android:name=".AddProduct" />
        <activity android:name=".ViewOrder" />
        <activity android:name=".Registration" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".Login" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>